<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.navbot">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".sing_in"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".profile"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".createpost"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".SettingsActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".cv_woker"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".my_notify"
            android:theme="@style/AppTheme.Dark" />
        <meta-data
            android:name="android.app.searchable"
            android:resource="@layout/searchable"></meta-data>
        <activity
            android:name=".main_jop"
            android:theme="@style/AppTheme.Dark" />
        <service android:name=".myservice" />

        <receiver android:name=".MyBroadCast"></receiver>
    </application>

</manifest>